                        <dt>
                            @Html.DisplayNameFor(model => model.NProt)
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.NProt)
                        </dd>
                        <dt>
                            Situação NFe
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.CSitNFe)
                        </dd>
                        <dt>
                            Tomador
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.CnpjTomador)
                        </dd>
                        <dt>
                            @Html.DisplayNameFor(model => model.Manifesta)
                        </dt>

                        <dt>
                            NSU
                        </dt>
                        <dd>
                            @Html.DisplayFor(model => model.NSU)
                        </dd>

                    </dl>
                </div>
                <a asp-action="Index" class="btn btn-primary">Voltar</a>
            </div>
        </div>
    </div>
</div>
